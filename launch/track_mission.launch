    <remap from="image" to="/usb_cam/image_raw"/>
    <param name="autosize" value="true" />
  </node> -->

<!--main python파일도 같이 실행
 <node pkg="vision_distance" type="track_mission.py" name="track_mission"/>
-->

</launch>